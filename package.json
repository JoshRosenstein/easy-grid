{
  "name": "easy-grid",
  "version": "0.0.5",
  "description": "A React component for utilizing CSS grid layout.",
  "scripts": {
    "build": "npm run clean && babel src/ -d build/ -s && npm run emotion",
    "clean": "rimraf build/ --no-glob",
    "test": "jest",
    "examples": "npm run build; cd examples; npm install; npm run build; npm run open",
    "emotion": "cross-env NODE_ENV=emotion babel src -d build/emotion -s"
  },
  "main": "build/grid.js",
  "keywords": [
    "css",
    "grid-layout",
    "grid",
    "layout",
    "react"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/google/easy-grid.git"
  },
  "author": "Joshua Harrison",
  "license": "Apache-2.0",
  "devDependencies": {
    "@babel/cli": "^7.1.5",
    "@babel/core": "^7.1.6",
    "@babel/preset-env": "^7.1.6",
    "@babel/preset-react": "^7.0.0",
    "@emotion/core": "^10.0.7",
    "@emotion/styled": "^10.0.7",
    "babel-jest": "^24.1.0",
    "babel-plugin-transform-rename-import": "^2.3.0",
    "cross-env": "^5.2.0",
    "enzyme": "^3.1.0",
    "enzyme-adapter-react-16": "^1.0.2",
    "jest": "^24.1.0",
    "jest-cli": "^24.1.0",
    "jest-emotion": "^10.0.7",
    "jest-styled-components": "^7.0.0-2",
    "react": "^16.8.3",
    "react-dom": "^16.8.3",
    "react-test-renderer": "^16.8.3",
    "regenerator-runtime": "^0.11.0",
    "rimraf": "^2.6.3",
    "styled-components": "^4.1.3"
  },
  "peerDependencies": {},
  "jest": {
    "testPathIgnorePatterns": [
      "/node_modules/"
    ],
    "testMatch": [
      "**/test/**/*.js"
    ],
    "transform": {
      "^.+\\.js?$": "babel-jest"
    }
  }
}
